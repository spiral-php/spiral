{
    "name": "spiral/prototype",
    "type": "library",
    "description": "Automatic dependency injection",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/prototype"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "nikic/php-parser": "^4.1",
        "doctrine/inflector": "^1.4|^2.0",
        "spiral/console": "^2.12",
        "spiral/attributes": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Prototype\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.5",
        "mockery/mockery": "^1.3.5",
        "cycle/orm": "^1.8.1",
        "spiral/debug": "^2.12"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Prototype\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.12.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
